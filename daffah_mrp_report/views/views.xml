<odoo>
    <data>
        <record id="view_mrp_production_form_inherit_daffah" model="ir.ui.view">
            <field name="name">mrp.production.form.inherit.daffah</field>
            <field name="model">mrp.production</field>
            <field name="inherit_id" ref="mrp.mrp_production_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='product_id']" position="after">
                    <field name="company_daffah"/>
                    <field name="projects_x"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>
